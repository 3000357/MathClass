{
  "name": "gametime-player",
  "description": "Simple libretro frontend",
  "author": "Matthew Bauer <mjbauer95@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/matthewbauer/gametime-player.git"
  },
  "main": "player.js",
  "jspm": {
    "main": "index.coffee",
    "directories": {},
    "dependencies": {
      "coffee": "github:forresto/system-coffee@^0.1.2",
      "document": "github:matthewbauer/document@^0.0.4",
      "gambatte": "npm:gambatte@^0.4.6",
      "gw": "npm:gw@^0.4.6",
      "jszip": "github:stuk/jszip@^2.5.0",
      "keypad": "github:matthewbauer/keypad@^0.0.2",
      "localforage": "github:mozilla/localforage@master",
      "nestopia": "npm:nestopia@^0.4.6",
      "snes9x-next": "npm:snes9x-next@^0.4.6",
      "sparkmd5": "github:satazor/sparkmd5@^1.0.0",
      "vba-next": "npm:vba-next@^0.4.6",
      "vecx": "npm:vecx@^0.4.6",
      "window": "github:matthewbauer/window@^0.0.3"
    },
    "devDependencies": {
      "traceur": "github:jmcriffey/bower-traceur@0.0.88",
      "traceur-runtime": "github:jmcriffey/bower-traceur-runtime@0.0.88"
    },
    "overrides": {
      "github:stuk/jszip@2.5.0": {
        "main": "dist/jszip.js"
      },
      "github:mohayonao/web-audio-api-shim@0.3.0": {
        "main": "build/web-audio-api-shim.js",
        "format": "global"
      },
      "github:webcomponents/webcomponentsjs@0.7.10": {
        "main": "webcomponents-lite.js"
      },
      "github:mozilla/localforage@master": {
        "main": "dist/localforage.nopromises.js",
        "format": "amd"
      }
    }
  },
  "devDependencies": {
    "chromedriver": "^2.16.0",
    "coffee-script": "^1.9.3",
    "html-inline": "^1.1.1",
    "html-minifier": "^0.7.2",
    "http-server": "^0.8.0",
    "jspm": "^0.15.7",
    "mocha": "^2.2.5",
    "selenium-webdriver": "^2.46.1",
    "systemjs-builder": "^0.12.2"
  },
  "scripts": {
    "serve": "http-server",
    "install": "jspm install -y",
    "test": "mocha test.js",
    "build": "jspm bundle 'index.coffee!' --minify --inject --skip-source-maps"
  }
}
